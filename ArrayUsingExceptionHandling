# assignment_in_java

package assignment_in_java;
import java.util.*;


public class ArrayUsingexceptionhandling {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
		try {
			
			ArrayList ae=new ArrayList();
		int[] arrNum= {5,3,4,2,0,8};
		
		int cummul=1;
		for(int i=0;i<arrNum.length;i++) {
			
			cummul=cummul*arrNum[i];
			arrNum[i]=cummul;
			
		}
		System.out.println(Arrays.toString(arrNum));
		
		}
		catch(Exception e) {
			e.printStackTrace();
		}
			
			

	}

}
